<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
        <!--  CSS Files -->
        
        <!-- Bootstrap -->
        <link href="/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
        
        <!-- PackingSheets -->
        <link href="/css/packingsheets.css" rel="stylesheet">
        
        <!-- JQuery UI -->
        <!-- <link href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css" rel="stylesheet"> -->
        <link href="/lib/jqueryui/jquery-ui.min.css" rel="stylesheet">  
               
        <!-- Select2 -->
        <!-- <link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet" /> -->
        <link href="/lib/select2/dist/css/select2.min.css" rel="stylesheet" />
        
        <!-- Zoomify -->
        <link href="/lib/zoomify/dist/zoomify.min.css" rel="stylesheet">
          
        <!-- Script Files -->
        
        <!-- JQuery -->
        <!--  <script src="https://code.jquery.com/jquery-3.1.1.js"></script> -->
        <script src="/lib/jquery/jquery-3.1.1.min.js"></script>
        
        <!-- Jquery UI --> 
  		<!-- <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>  -->
  		<script src="/lib/jqueryui/jquery-ui.min.js"></script>
  		
  		<!-- Bootstrap JS -->
  		<script src="/lib/bootstrap/js/bootstrap.min.js"></script>
  		
  		<!-- Select2 -->
  		<!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script> -->
		<script src="/lib/select2/dist/js/select2.min.js"></script>
		
		<!-- Zoomify -->
  		<script src="/lib/zoomify/dist/zoomify.min.js"></script>
  		
  		
        <title>PackingSheets - {% block title %}{% endblock %}</title>
    </head>
    <body>
        <div class='container'>
            {% if (app.session.get('user') != null) %}
                {% if (app.session.get('auth')['acredLevel'] == 3 )%}
                    {% include 'navbars/navbarAdmin.html.twig' %}
                {% elseif (app.session.get('auth')['acredLevel'] == 2) %}
                    {% include 'navbars/navbarSuperUser.html.twig' %}
                {% elseif (app.session.get('auth')['acredLevel'] == 1) %}
                    {% include 'navbars/navbarUser.html.twig' %}
                {% endif %}
            {% endif %}    
            <div id="content">{% block content %}{% endblock %}</div>
            <footer class="footer">
                Sabena Aerospace Engineering, 2016.
            </footer>
        </div>
    </body>
</html>
